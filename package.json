{
  "name": "dp-web-tests",
  "version": "1.0.0",
  "description": "Automated tests for dp websites",
  "main": "index.js",
  "scripts": {
    "start": "nightwatch --tag cmd; node create-html-report.js",
    "start:chrome": "nightwatch --tag cmd --env chrome; node create-html-report.js",
    "start:firefox": "nightwatch --tag cmd --env firefox; node create-html-report.js",
    "start:safari": "nightwatch --tag cmd --env safari; node create-html-report.js",
    "start:parallel-single-browser": "nightwatch --tag cmd --env chrome; node create-html-report.js",
    "start:parallel-multiple-browser": "nightwatch --tag cmd --env chrome,firefox; node create-html-report.js",
    "start:parallel-all-browser": "nightwatch --tag cmd --env chrome,firefox,safari; node create-html-report.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ONSdigital/dp-web-tests.git"
  },
  "keywords": [
    "automated",
    "testing",
    "web"
  ],
  "author": "ONSdigital",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ONSdigital/dp-web-tests/issues"
  },
  "homepage": "https://github.com/ONSdigital/dp-web-tests#readme",
  "dependencies": {
    "chromedriver": "^2.32.2",
    "cucumber": "^3.0.1",
    "cucumber-html-reporter": "^3.0.1",
    "edgedriver": "^4.15063.0",
    "geckodriver": "^1.8.1",
    "nightwatch": "^0.9.16",
    "nightwatch-allure-adapter": "^1.2.4",
    "nightwatch-cucumber": "^8.0.10",
    "phantomjs-prebuilt": "^2.1.15",
    "selenium": "^2.20.0",
    "selenium-server": "^3.5.3",
    "selenium-webdriver": "^3.5.0",
    "sigmund": "^1.0.1",
    "universalify": "^0.1.1"
  },
  "devDependencies": {}
}
